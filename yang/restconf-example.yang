module restconf-example {
  namespace "http://example.org/example-last-modified";
  prefix "ex";

  extension uci-package {
    argument name;
  }

  extension uci-section {
    argument name;
  }

  extension uci-option {
    argument name;
  }

  extension uci-section-type {
    argument name;
  }

  ex:uci-package "restconf";
  container state {
    ex:uci-section "state";
    ex:uci-section-type "state";

    leaf-list op {
      ex:uci-option "op";
      type int64;
    }

    leaf num {
      ex:uci-option "numa";
      type int8;
    }

    list people {
      ex:uci-section-type "people_list";
      key "name age";

      leaf name {
        ex:uci-option "name";
        type string;
      }

      leaf age {
        ex:uci-option "age";
        type int8;
      }

      leaf age2 {
        mandatory true;
        ex:uci-option "age2";
        type int8;
      }

      leaf-list age-list {
        ex:uci-option "age_list";
        type int64;
      }
    }

    container nested-state {
      ex:uci-section "nested_state";
      ex:uci-section-type "nested_state";

      leaf top {
        ex:uci-option "top";
        type string;
      }
    }
  }
}